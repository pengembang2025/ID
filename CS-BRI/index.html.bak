 function startCarousel(){
  var rootComp = 'slider-icon';
  $('.owl-carousel.slider-icon').each( function (index, item) {
    var rootIdClassName = 'owl-carousel.' + rootComp + ' slider-' + index;
    $(item).closest('.' + rootComp).addClass(rootIdClassName);
    $(item).owlCarousel({
      items : 2,
      dots:true,
      //dotsContainer: dot_cont,
      loop: false,
      margin: 30,
      nav: true,
      center: true,
      //navContainer: nav_cont,
      navText: [
