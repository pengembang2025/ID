$(document).ready(function() {
	$('input[name="_custom_search_portlet_keywords"]').on('keyup',function(e){
		var keycode=e.keyCode||e.which;
		if(parseInt(keycode)==13) {
			_custom_search_portlet_check();
		}
	});
});
function _custom_search_portlet_check(){
	$('.section-product-tab').addClass('has-load');
	var reg = /<script[^>]*>(?:(?!<\/script>)[^])*<\/script>/g;
	var htmlreg = /<(.|\n)*?>/g;
	var keywords = $('#_custom_search_portlet_keywords').val();
	var authToken=$('#_custom_search_portlet_authToken').val();
	var btnSearch = $('.btn-search').val();
	var errorText = "";
	if (reg.test(keywords) == true || 
		reg.test(authToken) == true || 
		reg.test(btnSearch) == true) {
		errorText ='do not allow SCRIPT TAGS';
	    alert(errorText);
	} else {
		if (htmlreg.test(keywords) == true || 
			htmlreg.test(authToken) == true || 
			htmlreg.test(btnSearch) == true) {
			errorText ='do not allow HTML TAGS';
		    alert(errorText);
		} else {
			_custom_search_portlet_searchContent();
		}
	}
}
