var waShare = $(document).find('[data-js="wa-share"]');
waShare.onclick = function(e) {
  e.preventDefault();
  var wa = window.open('whatsapp://send?text=' + encodeURIComponent(document.URL), 'wa-popup', 'height=350,width=600');
  if(wa.focus) { wa.focus(); }
    return false;
}
$(".open-share").click(function(){
  var content = $(this).siblings(".socmed");
  if(!$(this).hasClass("active")){
    $(content).fadeIn();
    $(this).addClass("active");
  }else{
    $(content).fadeOut();
    $(this).removeClass("active");
  }
});
$(".get-this-url").each(function(){
    var url = document.URL;
    $(this).attr("data-clipboard-text", url);
});
});
$('.btn-expand').on('click', function(){
var content  = $(this).parent().siblings(".special-text");
  $(content).toggleClass('-expanded');
  if ($(content).hasClass('-expanded')) {
    $(this).html('Lihat lebih sedikit');
  } else {
    $(this).html('Lihat lebih banyak');
  }
});
$('.go-to-div').click(function(e){
  e.preventDefault();
  var target = $($(this).attr('href'));
  $(".go-to-div").each(function () {
      $(this).removeClass('active');
  })
  $(this).addClass('active');
  if(target.length){
    var scrollTo = target.offset().top - 150;
    $('body, html').animate({scrollTop: scrollTo+'px'}, 1000);
  }
});
$(".CX-modal").click(function(evt) {
    if(!$(evt.target).closest('.CX-modal-inner').length){
      $(".CX-modal").addClass("hidden");
    }
});
